{
  "name": "reg-publish-gh-pages-plugin",
  "version": "0.2.2",
  "description": "A reg-suit publisher plugin to deploy visual regression test reports to GitHub Pages",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KyoheiG3/reg-publish-gh-pages-plugin.git"
  },
  "homepage": "https://github.com/KyoheiG3/reg-publish-gh-pages-plugin",
  "bugs": {
    "url": "https://github.com/KyoheiG3/reg-publish-gh-pages-plugin/issues"
  },
  "keywords": [
    "reg",
    "reg-suit",
    "reg-suit-plugin",
    "visual-regression",
    "vrt",
    "github-pages"
  ],
  "author": "KyoheiG3",
  "license": "MIT",
  "packageManager": "pnpm@10.30.2",
  "scripts": {
