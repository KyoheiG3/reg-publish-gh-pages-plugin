  "devDependencies": {
    "@types/node": "^25.0.5",
    "@vitest/coverage-v8": "^4.0.17",
    "dprint": "^0.52.0",
    "eslint": "^9.39.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "reg-suit-interface": "^0.14.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "vitest": "^4.0.17"
