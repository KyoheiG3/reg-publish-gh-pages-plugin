{
  "core": {
    "workingDir": ".reg",
    "actualDir": "__screenshots__",
    "thresholdRate": 0.001,
    "addIgnore": true,
    "ximgdiff": {
      "invocationType": "client"
    }
  },
  "plugins": {
    "reg-keygen-git-hash-plugin": {},
    "reg-publish-gh-pages-plugin": {
      "artifactDeploy": true,
      "branch": "gh-pages",
      "outDir": "$VRT_OUTPUT_DIR",
      "includeCommitHash": true
    },
    "reg-notify-github-plugin": {
      "clientId": "$REG_NOTIFY_CLIENT_ID"
    }
  }
}
